{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@fixtures": ["src/@fixtures"],
      "@fixtures/*": ["src/@fixtures/*"],
      "@types": ["src/@types"],
      "@types/*": ["src/@types/*"],
      "api": ["src/api"],
      "api/*": ["src/api/*"],
      "configs": ["src/configs"],
      "configs/*": ["src/configs/*"],
      "contexts": ["src/contexts"],
      "contexts/*": ["src/contexts/*"]
    }
  }
}
